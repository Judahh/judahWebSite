{
  "name": "judahh",
  "productName": "Judahh",
  "author": "Judah",
  "version": "0.1.0",
  "main": "App/MainElectron.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\"",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "build": "gulp build",
    "clean": "shx rm -Rf build/",
    "gulp": "gulp",
    "buildIonic": "ionic-app-scripts build",
    "watch": "ionic-app-scripts watch",
    "serve:before": "watch",
    "emulate:before": "buildIonic",
    "deploy:before": "buildIonic",
    "build:before": "buildIonic",
    "run:before": "buildIonic",
    "electronStart": "electron .",
    "tryWeb": "npm-run-all build lite",
    "tryDesktop": "npm-run-all build electron",
    "packDesktop": "electron-packager . --platform=all --arch=all --overwrite --icon=favicon.ico --ignore=node_modules/electron-*",
    "packDesktopX64": "electron-packager . --platform=all --arch=x64 --overwrite --icon=favicon.ico --ignore=node_modules/electron-*",
    "packDesktopWinX64": "electron-packager . --platform=win32 --arch=x64 --overwrite --icon=favicon.ico --ignore=node_modules/electron-*"
  },
  "dependencies": {
    "@angular/common": "~2.0.1",
    "@angular/compiler": "~2.0.1",
    "@angular/compiler-cli": "~0.6.3",
    "@angular/core": "~2.0.1",
    "@angular/forms": "~2.0.1",
    "@angular/http": "~2.0.1",
    "@angular/platform-browser": "~2.0.1",
    "@angular/platform-browser-dynamic": "~2.0.1",
    "@angular/router": "~3.0.1",
    "@angular/upgrade": "~2.0.1",
    "@angular/platform-server": "~2.0.1",
    "@ionic/storage": "1.0.3",
    "ionic-angular": "2.0.0-rc.1",
    "ionic-native": "2.2.3",
    "ionicons": "3.0.0",
    "angular-in-memory-web-api": "0.1.3",
    "chai": "^3.4.0",
    "core-js": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-mocha": "^2.1.3",
    "mocha": "^2.3.3",
    "ng2-responsive": "^0.4.7",
    "reflect-metadata": "^0.1.8",
    "run-sequence": "^1.1.4",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "dotenv": "^1.2.0",
    "electron": "^1.4.3",
    "electron-packager": "^7.3.0",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.1",
    "gulp-mocha": "^2.1.3",
    "gulp-postcss": "^6.1.1",
    "gulp-pug": "^3.0.3",
    "gulp-typescript": "^2.13.6",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "lite-server": "^2.2.2",
    "lost": "^7.0.3",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.3.0",
    "postcss-autoreset": "^1.1.5",
    "postcss-cli": "^2.5.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "pug": "^2.0.0-beta3",
    "pug-cli": "^1.0.0-alpha6",
    "shx": "^0.1.2",
    "@ionic/app-scripts": "^0.0.36",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "yargs": "^4.8.1"
  }
}
